<Project>
    <PropertyGroup>
        <SunkenlandGamePath>C:\Program Files (x86)\Steam\steamapps\common\Sunkenland</SunkenlandGamePath>
        <BepInExPath>$(SunkenlandGamePath)\BepInEx</BepInExPath>
        <CorlibPath>$(SunkenlandGamePath)\Sunkenland_Data\Managed</CorlibPath>
        <PublicizedAssembliesPath>$(SunkenlandGamePath)\Sunkenland_Data\Managed\publicized_assemblies</PublicizedAssembliesPath>
        <CopyOutputDLLPath>$(SunkenlandGamePath)\BepInEx\plugins</CopyOutputDLLPath> <!-- This is where the output DLL will be copied to after building. The templates support doing this for up to three locations. Duplicate this line and append a number. Example below -->
        <!--<CopyOutputDLLPath2>$(SunkenlandGamePath)\BepInEx\plugins</CopyOutputDLLPath2>-->
        <!--<CopyOutputDLLPath3>$(SunkenlandGamePath)\BepInEx\plugins</CopyOutputDLLPath3>-->
        <AfterTargets>AfterBuild</AfterTargets> <!-- If you are using KeyManager, change this to KeyManager as suggested in step 5.2 in the instructions -->
    </PropertyGroup>
</Project>